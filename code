const flipIconImage = titleContainer.children.push(am5.Picture.new(root, {
      src: flipIcon,
      width: 20,
      height: 20,
      marginLeft: 5,
      centerY: am5.p50,
    }));
    // Tooltip for flip icon
    const flipIconTooltip = am5.Tooltip.new(root, {
      labelText: "Click on the dial to flip",
      autoTextColor: false,
    });
    flipIconTooltip.get("background").setAll({
      fill: am5.color("#000000"),
      fillOpacity: 1,
    });
    flipIconTooltip.label.setAll({
      fontFamily: "kenvue-sans",
      fontSize: 13.33,
      fontWeight: "400",
      fill: am5.color("#F8F8F8"),
      lineHeight: 19.99,
      paddingTop: 12,
      paddingRight: 20,
      paddingBottom: 12,
      paddingLeft: 20,
    });
    flipIconImage.set("tooltip", flipIconTooltip);

